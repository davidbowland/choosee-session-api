{
  "name": "choosee-session-api",
  "version": "1.33.0",
  "description": "Lambda choosee session API",
  "author": "David Bowland",
  "engines": {
    "node": "^20.0.0"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davidbowland/choosee-session-api.git"
  },
  "bugs": {
    "url": "https://github.com/davidbowland/choosee-session-api/issues"
  },
  "homepage": "https://github.com/davidbowland/choosee-session-api#readme",
  "scripts": {
    "clean": "rm -rf .aws-sam coverage && NODE_ENV=test npm ci",
    "deploy": "NODE_ENV=production npm ci && ./scripts/deploy.sh",
    "lint": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "start": "./scripts/runLocal.sh",
    "test": "jest --colors",
    "typecheck": "tsc --noEmit",
    "update": "ncu --doctor --target minor --upgrade && npm audit fix --audit-level=none && npm run test && npm dedupe"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.682.0",
    "@googlemaps/google-maps-services-js": "^3.4.0",
    "aws-xray-sdk-core": "^3.10.1",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "esbuild": "^0.24.0",
    "fast-json-patch": "^3.1.1",
    "husky": "^9.1.6",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.17.5",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.8.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "msw": "^2.6.0",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --fix",
      "jest --colors --findRelatedTests --coverageThreshold '{}' --passWithNoTests"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  }
}
